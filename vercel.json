{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "github": {
        "autoJobCancelation": true,
        "silent": true,
        "autoAlias": true,
        "enabled": true
    },
    "git": {
        "deploymentEnabled": {
            "master": true
        }
    }
}